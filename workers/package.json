{
  "name": "goal-portfolio-sync-workers",
  "version": "1.2.0",
  "description": "Cloudflare Workers backend for Goal Portfolio Viewer sync",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "tail": "wrangler tail",
    "tail:staging": "wrangler tail --env staging",
    "tail:production": "wrangler tail --env production",
    "kv:create": "wrangler kv:namespace create SYNC_KV",
    "kv:list": "wrangler kv:namespace list",
    "test": "node --test test/*.test.js",
    "test:unit": "node --test test/*.test.js",
    "test:unit:coverage": "c8 --reporter=json-summary --reporter=text --check-coverage --lines 70 --functions 65 --branches 60 --statements 70 node --test test/*.test.js"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "sync",
    "privacy"
  ],
  "author": "laurenceputra",
  "license": "MIT",
  "devDependencies": {
    "c8": "^10.1.3",
    "wrangler": "^4.62.0"
  }
}
